##############
# Statements #
##############

SET Citation = {"PubMed", "Thromb Res. 2015 Oct;136(4):769-74. doi: 10.1016/j.thromres.2015.08.017. Epub 2015 Aug 28.", "26337933"}

SET TextLocation = "Introduction"

SET Evidence = "Humans with severe PE have increased arginase-1 
and decreased plasma L-arginine concentrations.[14]"

p(HGNC:ARG1) pos path(MESH:"Pulmonary Embolism")
a(CHEBI:"L-arginine") neg path(MESH:"Pulmonary Embolism")

SET TextLocation = "Results"

SET Species = "Homo sapiens"

SET Evidence = "The median values and associated interquartile 
ranges for hmox-1 and haptoglobin show that patients with 
PE+TR+ had lower expression of these genes compared with 
patients who were PE+TR− or PE−"

SET MeSHDisease = "Pulmonary Embolism"
path(MESH:"Tricuspid Valve Insufficiency") neg p(HGNC:HMOX1)
path(MESH:"Tricuspid Valve Insufficiency") neg p(HGNC:HP)

SET Evidence = "The cyclooxygenase-2 gene was not significantly 
different between groups and had no correlation with the TR jet 
velocity in patients with PE (R2=0.03)."

SET MeSHDisease = "Pulmonary Embolism"
path(MESH:"Tricuspid Valve Insufficiency") causesNoChange g(HGNC:PTGS2)

SET Evidence = "Table 3 shows a significant increase in the 
median value for the plasma biomarker of hemolysis, 
arginase-1 in patients with PE+TR+ by pairwise comparison 
using the Kruskal-Wallis test."

p(HGNC:ARG1) pos path(MESH:Hemolysis)
SET MeSHDisease = "Pulmonary Embolism"
path(MESH:"Tricuspid Valve Insufficiency") pos p(HGNC:ARG1)

SET Evidence = "The inverse correlation between hmox-1 
expression in circulating leukocytes and the TR jet 
velocity in patients with acute PE was significant 
(r2=0.45, p <0.001), but this significance was 
absent in patients without PE (r2=0.01, P=0.52)."

SET Cell = "leukocyte"
SET MeSHDisease = "Pulmonary Embolism"
path(MESH:"Tricuspid Valve Insufficiency") neg p(HGNC:HMOX1)

SET TextLocation = "Discussion"

SET Evidence = "We hypothesize acute moderate to severe
PE causes turbulent flow across the tricuspid and 
pulmonic valves and in the pulmonary tree, causing 
rupture of a small percentage of red cells in or 
immediately proximal to or within the pulmonary 
vascular tree."

path(MESH:"Pulmonary Embolism") pos path(MESH:Hemolysis)

SET Evidence = "Release of hemoglobin (and ultimately heme), 
and arginase-1 into the plasma results in binding of nitric 
oxide and also reduction in L-arginine, respectively, 
causing a relative reduction in vasodilatory effect.[14,29]"

SET MeSHAnatomy = "Plasma"
p(HGNC:HBB) pos complex(p(HGNC:HBB), a(CHEBI:"nitric oxide"))
p(HGNC:HBB) -| a(CHEBI:"L-arginine")

UNSET TextLocation 
